    "@opam/mirage-runtime": "3.7.6",
    "@opam/mirage-types": "3.7.6",
    "@opam/mirage-types-lwt": "3.7.6",
    "@opam/mirage-unix": "4.0.0",
    "@opam/reason": "3.6.0",
    "ocaml": "4.9.0"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam",
