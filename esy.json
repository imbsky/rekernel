{
  "name": "rekernel",
  "version": "0.0.0",
  "description": "A minimal setup for writing Unikernels in ReasonML",
  "repository": "https://github.com/imbsky/rekernel",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "esy": {
